CREATE MEMORY TABLE COMMENT(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,POSTER_CLASS VARCHAR(255) NOT NULL,LAST_UPDATED TIMESTAMP NOT NULL,POSTER_ID BIGINT NOT NULL,DATE_CREATED TIMESTAMP NOT NULL,BODY LONGVARCHAR NOT NULL)
