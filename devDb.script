CREATE MEMORY TABLE USER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,PASSWD VARCHAR(255) NOT NULL,EMAIL_SHOW BOOLEAN NOT NULL,USER_REAL_NAME VARCHAR(255) NOT NULL,ENABLED BOOLEAN NOT NULL,DESCRIPTION VARCHAR(255) NOT NULL,USERNAME VARCHAR(255) NOT NULL,EMAIL VARCHAR(255) NOT NULL,CONSTRAINT SYS_CT_61 UNIQUE(USERNAME))
CREATE MEMORY TABLE USER_USER(USER_FOLLOWING_ID BIGINT,USER_ID BIGINT,CONSTRAINT FK143D5FBFF7634DFA FOREIGN KEY(USER_ID) REFERENCES USER(ID),CONSTRAINT FK143D5FBF5424C968 FOREIGN KEY(USER_FOLLOWING_ID) REFERENCES USER(ID))
CREATE MEMORY TABLE STATUS(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,MESSAGE VARCHAR(140) NOT NULL,QUACKER_ID BIGINT NOT NULL,DATE_CREATED TIMESTAMP NOT NULL,CONSTRAINT FKCACDCFF225900913 FOREIGN KEY(QUACKER_ID) REFERENCES USER(ID))
CREATE MEMORY TABLE STATUS_USER(STATUS_LIKED_ID BIGINT,USER_ID BIGINT,CONSTRAINT FKCAE1FBF8F7634DFA FOREIGN KEY(USER_ID) REFERENCES USER(ID),CONSTRAINT FKCAE1FBF83711E0AC FOREIGN KEY(STATUS_LIKED_ID) REFERENCES STATUS(ID))
